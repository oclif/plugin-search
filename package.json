{
  "name": "@oclif/plugin-search",
  "version": "1.0.12",
  "description": "A command for searching commands",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-search/issues",
  "dependencies": {
    "@oclif/core": "^3.18.1",
    "inquirer-autocomplete-standalone": "^0.8.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.1.12",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18",
    "chai": "^4",
    "commitlint": "^17.8.1",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.35",
    "eslint-config-prettier": "^9.1.0",
    "globby": "^11",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "oclif": "^4.3.6",
    "prettier": "^3.2.4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/oclif.lock"
  ],
  "homepage": "https://github.com/oclif/plugin-search",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./dist/commands",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "flexibleTaxonomy": true
  },
  "repository": "oclif/plugin-search",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json oclif.lock",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif lock && oclif manifest && oclif readme",
    "prepare": "husky install && yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
